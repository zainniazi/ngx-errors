{
  "name": "@hackages/ngxerrors",
  "version": "8.0.0",
  "description": "A declarative validation module for reactive forms forked from UltimateAngular/ngx-errors",
  "author": "Hackages",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/hackages/ngx-errors.git",
    "type": "git"
  },
  "scripts": {
    "build": "yarn run clean && yarn run copy && yarn run compile",
    "clean": "rimraf dist && mkdir -p dist",
    "compile": "yarn run ngc && yarn run rollup",
    "copy": "cp src/package.json dist/package.json && cp README.md dist/README.md",
    "example": "webpack-dev-server --inline --hot",
    "prestart": "yarn run clean && yarn run copy",
    "start": "nodemon --exec yarn run compile -e ts --watch src",
    "rollup": "rollup -c rollup.config.js >/dev/null 2>&1",
    "ngc": "ngc -p tsconfig.json",
    "test": "karma start --single-run",
    "test:watch": "karma start"
  },
  "devDependencies": {
    "@angular/compiler": "11.0.0",
    "@angular/compiler-cli": "11.0.0",
    "@types/jasmine": "3.6.1",
    "@types/karma": "5.0.1",
    "@types/node": "^14.14.7",
    "angular2-template-loader": "0.6.2",
    "awesome-typescript-loader": "5.2.1",
    "file-loader": "6.2.0",
    "html-loader": "1.3.2",
    "jasmine-core": "3.6.0",
    "karma": "5.2.3",
    "karma-chrome-launcher": "3.1.0",
    "karma-jasmine": "4.0.1",
    "karma-sourcemap-loader": "0.3.8",
    "karma-spec-reporter": "0.0.32",
    "karma-webpack": "4.0.2",
    "node-sass": "^5.0.0",
    "nodemon": "2.0.6",
    "raw-loader": "4.0.2",
    "rimraf": "3.0.2",
    "rollup": "2.33.2",
    "sass-loader": "10.1.0",
    "typescript": "4.0.5",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "5.4.0",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "3.11.0"
  },
  "dependencies": {
    "@angular/animations": "11.0.0",
    "@angular/common": "11.0.0",
    "@angular/core": "11.0.0",
    "@angular/forms": "11.0.0",
    "@angular/platform-browser": "11.0.0",
    "@angular/platform-browser-dynamic": "11.0.0",
    "@angular/router": "11.0.0",
    "core-js": "3.7.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.3",
    "tslib": "2.0.3",
    "zone.js": "0.11.3"
  }
}
